{
  "eslint.enable": true,
  "eslint.validate": [
    "javascript", 
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.rules.customizations": [
    { "rule": "*", "severity": "warn" }
  ]
}